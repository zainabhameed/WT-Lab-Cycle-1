<form action="">
    Enter a Number between 1 to 50: <input type="number" name="num" min=1 max=50 ><br><br>
    <input type="submit"><br>
</form>
<?php
$num = 1 ;
while ($num<=$_GET["num"]  ) {
    $count=0;
    for ( $i=1;$i<=$num;$i++) {
        if (($num%$i)==0) {
            $count++;
        }
    }
    if ($count==2 ) {
    echo $num." , ";
    }
     $num=$num+1;
}
